{
  "name": "eslint-config-intellibus",
  "description": "ESLint Shareable Config for Intellibus Labs",
  "version": "1.0.78",
  "bugs": {
    "url": "https://github.com/intellibus/eslint-config-intellibus/issues"
  },
  "homepage": "https://github.com/intellibus/eslint-config-intellibus",
  "keywords": [
    "eslint",
    "eslintconfig",
    "intellibus"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && (pnpm run esbuild & pnpm run typedef)",
    "esbuild": "esbuild src/*.ts --minify --outdir=dist --platform=node --target=node14 --format=cjs",
    "typedef": "tsc -d src/*.ts --declarationDir dist --emitDeclarationOnly",
    "prepublish": "pnpm run build",
    "test": "uvu -r tsm test",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.2",
    "eslint-plugin-no-only-tests": "2.6.0",
    "eslint-plugin-promise": "6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@commitlint/types": "17.0.0",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/eslint": "8.4.6",
    "@types/node": "18.0.0",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.15.11",
    "eslint": "8.25.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "semantic-release-interval": "1.0.46",
    "svelte": "3.52.0",
    "tsm": "2.2.2",
    "typescript": "4.8.4",
    "uvu": "0.5.6"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": "./dist"
  }
}
